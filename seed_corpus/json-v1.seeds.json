{
  "schema_version": 1,
  "dataset_id": "json-v1",
  "target_family": "json",
  "notes": "Balanced by bucket to shape early reward diversity for parser and decoder targets.",
  "buckets": [
    {
      "name": "valid",
      "description": "Syntactically valid JSON with varied structures and scalar types."
    },
    {
      "name": "string_stress",
      "description": "Inputs emphasizing escapes, unicode, and long strings (mostly valid)."
    },
    {
      "name": "near_valid",
      "description": "Small edits away from valid JSON to trigger parse-edge diagnostics."
    }
  ],
  "seeds": [
    {
